<center> <h1>Student Form</h1>
  <%= form_for @student do |f| %>
    <br><br>
    <%=f.text_field :first_name %><br><br>
    <%=f.text_field :last_name %><br><br>
    <%=f.date_field :birth_date, start_date: Date.current.year, end_date: Date.current.year - 1 %><br><br>
    <%=f.submit %>
  <% end %>
</center>